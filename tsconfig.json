{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["es2015", "es6", "dom"],
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "incremental": true
  },
  "include": ["jest-extended.d.ts"]
}
